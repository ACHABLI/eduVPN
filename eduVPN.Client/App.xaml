<Application
    x:Class="eduVPN.Client.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    StartupUri="Views/ConnectWizard.xaml">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Constants.xaml"/>

                <ResourceDictionary Source="Resources/Converters.xaml"/>

                <ResourceDictionary Source="Resources/Colors.xaml"/>
                <ResourceDictionary Source="Resources/Brushes.xaml"/>

                <ResourceDictionary Source="Resources/ExclamationIcon.xaml"/>
                <ResourceDictionary Source="Resources/OpenVPNStateTypeIconConnecting.xaml"/>
                <ResourceDictionary Source="Resources/OpenVPNStateTypeIconWaiting.xaml"/>
                <ResourceDictionary Source="Resources/OpenVPNStateTypeIconAuthenticating.xaml"/>
                <ResourceDictionary Source="Resources/OpenVPNStateTypeIconGettingConfiguration.xaml"/>
                <ResourceDictionary Source="Resources/OpenVPNStateTypeIconAssigningIP.xaml"/>
                <ResourceDictionary Source="Resources/OpenVPNStateTypeIconAddingRoutes.xaml"/>
                <ResourceDictionary Source="Resources/OpenVPNStateTypeIconConnected.xaml"/>
                <ResourceDictionary Source="Resources/OpenVPNStateTypeIconReconnecting.xaml"/>
                <ResourceDictionary Source="Resources/OpenVPNStateTypeIconExiting.xaml"/>
                <ResourceDictionary Source="Resources/OpenVPNStateTypeIconFatalError.xaml"/>
                <ResourceDictionary Source="Resources/OpenVPNStateTypeInitializingIcon.xaml"/>
                <ResourceDictionary Source="Resources/TwoFactorAuthenticationIcon.xaml"/>
                <ResourceDictionary Source="Resources/UserDisabledIcon.xaml"/>

                <ResourceDictionary Source="Resources/Styles.xaml"/>
                <ResourceDictionary Source="Resources/DataTemplates.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
